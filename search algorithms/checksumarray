def checksumarray(aray,size,x):
    for i in range(0,size-1):
        for j in range(i+1,size):
            if ((aray[i]+aray[j]) == x):
                return -1

        return 0

arry =[ 1,2,5,4]
x = 3
size = len(arry)
if (checksumarray(arry, size, x)):
        print("Yes")
 
else:
    print("No")